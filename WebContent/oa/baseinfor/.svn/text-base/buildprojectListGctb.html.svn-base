<#include "/sysbase/common/include.html"/>
<title></title>
<script type="text/javascript" src="${basePath}/oa/baseinfor/buildprojectListGctb.js"></script>
<script type="text/javascript">
var onkeyup_prevTime = new Date().getTime();
function onkeyupSearch(){
	if(new Date().getTime()-onkeyup_prevTime > 500){
		setTimeout("search();",500);
	}
	onkeyup_prevTime = new Date().getTime();
}
</script>
</head>
<body class="easyui-layout" data-options="fit:true,border:false">
	<div id="toolbar" style="display: none;">
		<table border="0">
			<tr>
				<td style="padding-left: 10px;"><a id="btn" href="#"
					class="easyui-linkbutton" onClick="add();"
					data-options="iconCls:'icon-add'">工程填报</a></td>
				<td style="padding-left: 10px;"><a id="btn" href="#"
					class="easyui-linkbutton" onClick="updateBuildProject();"
					data-options="iconCls:'icon-add'">编辑</a></td>
				<td style="padding-left: 10px;"><a id="btn" href="#"
					class="easyui-linkbutton" onClick="submit();"
					data-options="iconCls:'icon-add'">提交</a></td>
			</tr>
		</table>
		<form id="searchForm">
			<table border="0">
				<tr>
					<td>工程名称:</td>
					<td><input name="proName" id="proName" onkeyup="onkeyupSearch();" placeholder="支持拼音首字母查询"  style="width:130px;" />
					</td>
					<td style="padding-left: 10px;"><a id="btn"
						href="javascript:search();" class="easyui-linkbutton"
						data-options="iconCls:'icon-search'">查询</a></td>
					<td style="padding-left: 10px;"><a id="btn"
						href="javascript:resetSearch();" class="easyui-linkbutton"
						data-options="iconCls:'icon-redo'">重置</a></td>
				</tr>
			</table>
		</form>
	</div>
	<div data-options="region:'center',fit:true,border:false">
		<table id="dataGrid" data-options="fit:true,border:false"></table>
	</div>
</body>
</html>
