<#include "/sysbase/common/include.html"/>
<title>Insert title here</title>
<script type="text/javascript" src="${basePath}/resources/js/IncJs.a?c=yes_no"></script>
<script type="text/javascript" src="${basePath}/oa/baseinfor/applyMajorEdit.js"></script>
</head>
<body class="easyui-layout" data-options="fit:true,border:false">
	<form id="submitForm">
	<input type="hidden" name="apply_major_id" value="${apply_major_id!''}"/>
		<table border="0" cellspacing="0" cellpadding="0" style="width:100%;"
			class="formTable">
			<tr>
				<td style="width: 120px;" align="right">操作:</td>
				<td align="left">
					<a href="javascript:void(0);" class="easyui-linkbutton" iconCls="icon-save" onclick="save()">保存</a> 
					<a href="javascript:void(0);" class="easyui-linkbutton" iconCls="icon-cancel" onclick="javascript:Dialog.close();">取消</a>
				</td>
			</tr>
				<tr>
					<td style="width: 80px;" align="right">专项名称:</td>
					<td><input type="text" name="major_name" value="${major_name!''}"  style="width: 280px;"
						class="easyui-validatebox class1" data-options="required:true" /></td>
				</tr>
				<tr>
					<td style="width: 80px;" align="right">工程类型:</td>
					<td><input type="text" name="apply_pro_type" value="${apply_pro_type!''}" style="width: 150px;"
						class="easyui-validatebox class1" data-options="required:true" /></td>
				</tr>
				<tr>
					<td style="width: 80px;" align="right">关联角色:</td>
					<td><input type="text" name="relation_role" value="${relation_role!''}" style="width: 150px;"
						class="easyui-validatebox class1" data-options="required:true" /></td>
				</tr>
				<tr>
					<td style="width: 80px;" align="right">排序:</td>
					<td><input type="text" name="order_code" value="${order_code!''}" style="width: 150px;"
						class="easyui-validatebox class1" data-options="required:true" /></td>
				</tr>
		</table>
	</form>
</body>
</html>
