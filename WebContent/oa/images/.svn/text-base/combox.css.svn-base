
.combox
{
    background:#fff;
    border:1px #D9C7A7 solid;
    position:relative; 
    float: left;
    margin: 4px;
    width: 400px;
    height: 200px;
}

.combox h2
{
    background: url(combox_title.png);
	line-height:30px;
	height:30px;
	font-size: 14px;
    padding-left: 10px;
	border-bottom: 1px solid #D9C7A7;
	margin: 0;
}


.combox .content
{
	padding:6px;
	overflow:auto;
	height: 158px;
}
