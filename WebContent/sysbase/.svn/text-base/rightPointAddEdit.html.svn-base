<#include "/sysbase/common/include.html"/>
<title>Insert title here</title>
<script type="text/javascript" >
//保存
function save() {
	if ($('#submitForm').form('validate')) {
		var url = basePath + '/sysbase/rightPointAddEditSave.c';
		$('#submitForm').form('submit', {
			url : url,
			success : function(result) {
				var result = eval('(' + result + ')');
				if (result.isSuccess) {
					alert(result.msg);
					//回调父页面查询方法，刷新datagrid
					getParentWindow().search();
					//关闭本弹出窗口
					Dialog.close();
				} else {
					alert(result.msg);
				}
			}
		});
	}
}
</script>
</head>
<body class="easyui-layout" data-options="fit:true,border:false">
	<form id="submitForm">
		<input type="hidden" name="pCode" value="${pCode!''}"/>
		<input type="hidden" name="rightId" value="${rightId!''}"/>
		<input type="hidden" name="addOrEdit" value="${addOrEdit!''}"/>
		<table border="0" cellspacing="0" cellpadding="0" style="width:100%;"
			class="formTable">
			<tr>
				<td style="width: 80px;" align="right">操作:</td>
				<td align="left">
					<a href="javascript:void(0);" class="easyui-linkbutton"
						iconCls="icon-save" onclick="save()">保存</a> <a
						href="javascript:void(0);" class="easyui-linkbutton"
						iconCls="icon-cancel"
						onclick="javascript:Dialog.close();">取消</a>
				</td>
			</tr>
				<tr>
					<td class="label" style="width: 80px;" align="right">权限名称:</td>
					<td><input id="rightName" name="rightName"  value="${editObj.rightName!''}"
							class="easyui-validatebox class1" data-options="required:true" 
							type="text" style="width: 150px;"/></td>
				</tr>
				<tr>
					<td class="label" style="width: 80px;" align="right">权限编码:</td>
					<td><input id="rightCode" name="rightCode"  value="${editObj.rightCode!''}"
							class="easyui-validatebox class1" data-options="required:true" 
							type="text" style="width: 150px;"/></td>
				</tr>
				<tr>
					<td class="label" style="width: 80px;" align="right">排序:</td>
					<td><input id="rightCode" name="orderCode"  value="${editObj.orderCode!''}"
							class="easyui-validatebox class1" data-options="required:true" 
							type="text" style="width: 150px;"/></td>
				</tr>
		</table>
	</form>
</body>
</html>
